"        ${s.name}</br>\n"
"        %else\n"
"        ${s.name},\n"
"        %endif\n"
"\n"
"        %endfor\n"
"       % endif\n"
"       % if object.category_id.name == 'Hotel':\n"
"       <table>\n"
"       <thead>\n"
"        <caption style=text-align=center; align=top><h3>Rooming</h3>\n"
"        </caption>\n"
