#. module: travel_core
#: model:email.template,body_html:travel_core.core_supplier_email_template
msgid "\n"
"<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"\n"
"    <p>Estimado  ${object.supplier_id.name},</p>\n"
"\n"
"    <p>Le solicitamos el siguiente servicio: </p>\n"
"\n"
"    <p style=\"border-left: 1px solid #8e0000; margin-left: 30px;\">\n"
"       &nbsp;&nbsp;${object.category_id.name}: <strong>${object.product_id.name}</strong><br />\n"
"       &nbsp;&nbsp;Fecha de Inicio: <strong>${object.start_date}</strong><br />\n"
"       &nbsp;&nbsp;Fecha de Fin: <strong>${object.end_date}</strong><br />\n"
"       &nbsp;&nbsp;Cantidad: <strong>${object.product_uom_qty| int}</strong><br />\n"
"       % if object.hotel_2_meal_plan_id:\n"
"       &nbsp;&nbsp;Plan: ${object.hotel_2_meal_plan_id.name}<br />\n"
"       % endif\n"
"       % if object.category_id.name != 'Hotel':\n"
"       &nbsp;&nbsp;Adultos: ${object.adults}<br />\n"
"       &nbsp;&nbsp;Niños: ${object.children}<br />\n"
"       % endif\n"
"       % if object.category_id.name == 'Transfer':\n"
"       &nbsp;&nbsp;Tipo de Transporte: ${object.transfer_1_vehicle_type_id.name}<br />\n"
"       &nbsp;&nbsp;Guía: ${object.transfer_2_guide_id.name}<br />\n"
