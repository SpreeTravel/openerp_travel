<?xml version="1.0" ?>
<openerp>
	<data>
	
		<record id="8transmission" model="product.attribute">
			<field name="name">Transmission</field>
			<field name="type">distinct</field>
		</record>
		<record id="8transmission_automatic" model="product.attribute.value">
			<field name="attribute_id" ref="8transmission"/>
			<field name="name">Automatic</field>
		</record>
		<record id="8transmission_mechanic" model="product.attribute.value">
			<field name="attribute_id" ref="8transmission"/>
			<field name="name">Mechanic</field>
		</record>		
		<record id="8class" model="product.attribute">
			<field name="name">Class</field>
			<field name="type">distinct</field>
		</record>
		<record id="8class_sedan" model="product.attribute.value">
			<field name="attribute_id" ref="8class"/>
			<field name="name">Sedan</field>
		</record>
		<record id="8class_compact" model="product.attribute.value">
			<field name="attribute_id" ref="8class"/>
			<field name="name">Compact</field>
		</record>
		<record id="8class_van" model="product.attribute.value">
			<field name="attribute_id" ref="8class"/>
			<field name="name">Van</field>
		</record>		
		<record id="8passengers" model="product.attribute">
			<field name="name">Passengers</field>
			<field name="type">float</field>
		</record>
		
		<!-- Old data -->

		<record id="option_type_transmission" model="option.type">
			<field name="code">tm</field>
			<field name="name">Transmission</field>
		</record>
		<record id="transmission_automatic" model="option.value">
			<field name="option_type_id" ref="option_type_transmission"/>
			<field name="code">automatic</field>
			<field name="name">AUTOMATIC</field>
		</record>
		<record id="transmission_mechanic" model="option.value">
			<field name="option_type_id" ref="option_type_transmission"/>
			<field name="code">mechanic</field>
			<field name="name">MECHANIC</field>
		</record>

		<record id="option_type_class" model="option.type">
			<field name="code">cl</field>
			<field name="name">Class</field>
		</record>
		<record id="sedan" model="option.value">
			<field name="option_type_id" ref="option_type_class"/>
			<field name="code">sedan</field>
			<field name="name">SEDAN</field>
		</record>
		<record id="compact" model="option.value">
			<field name="option_type_id" ref="option_type_class"/>
			<field name="code">compact</field>
			<field name="name">COMPACT</field>
		</record>
		<record id="van" model="option.value">
			<field name="option_type_id" ref="option_type_class"/>
			<field name="code">van</field>
			<field name="name">VAN</field>
		</record>
	</data>
</openerp>
